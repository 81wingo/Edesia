<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.edesia.presentation">

    <application
        android:supportsRtl="true"
        android:allowBackup="false"
        tools:targetApi="donut"
        tools:ignore="ExtraText,GoogleAppIndexingWarning"
        android:icon="@mipmap/ic_launcher_sq_grape"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_circle_grape"
        android:theme="@style/AppTheme"
        android:value="@navigation/navi_graph" >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MealRandomizer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>